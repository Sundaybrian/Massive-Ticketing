{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "TicketSubtype",
    "description": "The TicketSubtype",
    "required": ["name", "ticket_type_id"],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the TicketSubtype."
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of the subtype"
        },
        "ticket_type_id": {
            "$id": "#/properties/ticket_type_id",
            "type": "number",
            "title": "The id of the parent ticket type."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the user."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the user was last updated."
        }
    }
}
